<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintmainscreenreisen"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/rcreme"
    tools:context=".activities.MainScreenReisenActivity">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.075" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="76dp"
        android:layout_height="91dp"
        android:layout_marginStart="30dp"
        app:layout_constraintBottom_toBottomOf="@id/guideline14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/guideline14"
        app:layout_constraintVertical_bias="0.494"
        app:srcCompat="@drawable/smallbuttondots" />

    <ImageView
        android:id="@+id/imageViewSecond"
        android:layout_width="76dp"
        android:layout_height="91dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@id/guideline14"
        app:layout_constraintStart_toEndOf="@id/imageView4"
        app:layout_constraintTop_toTopOf="@id/guideline14"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/smallbuttondots" />

    <ImageView
        android:id="@+id/imageViewThird"
        android:layout_width="76dp"
        android:layout_height="91dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintBottom_toBottomOf="@id/guideline14"
        app:layout_constraintEnd_toStartOf="@+id/imageViewFourth"
        app:layout_constraintStart_toEndOf="@id/imageViewSecond"
        app:layout_constraintTop_toTopOf="@id/guideline14"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/smallbuttondots" />

    <ImageView
        android:id="@+id/imageViewFourth"
        android:layout_width="76dp"
        android:layout_height="91dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintBottom_toBottomOf="@id/guideline14"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintTop_toTopOf="@id/guideline14"
        app:layout_constraintVertical_bias="0.505"
        app:srcCompat="@drawable/smallbuttondots" />


    <ImageView
        android:id="@+id/imageView3Aktivity"
        android:layout_width="40dp"
        android:layout_height="33dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@id/imageViewThird"
        app:layout_constraintStart_toStartOf="@id/imageViewThird"
        app:layout_constraintTop_toTopOf="@id/imageViewThird"
        app:srcCompat="@drawable/bookmarkicon" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="49dp"
        android:layout_height="34dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@id/imageView4"
        app:layout_constraintStart_toStartOf="@id/imageView4"
        app:layout_constraintTop_toTopOf="@id/imageView4"
        app:srcCompat="@drawable/burgericon" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="@id/imageViewSecond"
        app:layout_constraintStart_toStartOf="@id/imageViewSecond"
        app:layout_constraintTop_toTopOf="@id/imageViewSecond"
        app:srcCompat="@drawable/clipboardicon" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="48dp"
        android:layout_height="12dp"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/cmedium"
        android:gravity="center"
        android:text="Ausgaben"
        android:textColor="@color/rcreme"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@id/imageView4"
        app:layout_constraintEnd_toEndOf="@id/imageView4"
        app:layout_constraintStart_toStartOf="@id/imageView4" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="48dp"
        android:layout_height="12dp"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/cmedium"
        android:gravity="center"
        android:text="Packliste"
        android:textColor="@color/rcreme"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@id/imageViewSecond"
        app:layout_constraintEnd_toEndOf="@id/imageViewSecond"
        app:layout_constraintStart_toStartOf="@id/imageViewSecond" />

    <TextView
        android:id="@+id/textView3Aktivity"
        android:layout_width="wrap_content"
        android:layout_height="12dp"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/cmedium"
        android:gravity="center"
        android:text="Dokumente"
        android:textColor="@color/rcreme"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@id/imageViewThird"
        app:layout_constraintEnd_toEndOf="@id/imageViewThird"
        app:layout_constraintStart_toStartOf="@id/imageViewThird" />

    <ImageView
        android:id="@+id/imageView5Aktivity"
        android:layout_width="39dp"
        android:layout_height="31dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="@id/imageViewFourth"
        app:layout_constraintHorizontal_bias="0.562"
        app:layout_constraintStart_toStartOf="@id/imageViewFourth"
        app:layout_constraintTop_toTopOf="@id/imageViewFourth"
        app:srcCompat="@drawable/pinpoint" />

    <TextView
        android:id="@+id/textView5Aktivity"
        android:layout_width="48dp"
        android:layout_height="12dp"
        android:layout_marginBottom="30dp"
        android:fontFamily="@font/cmedium"
        android:gravity="center"
        android:text="Stopps"
        android:textColor="@color/rcreme"
        android:textSize="10dp"
        app:layout_constraintBottom_toBottomOf="@id/imageViewFourth"
        app:layout_constraintEnd_toEndOf="@id/imageViewFourth"
        app:layout_constraintStart_toStartOf="@id/imageViewFourth" />

    <EditText
        android:id="@+id/pinTextView"
        android:layout_width="346dp"
        android:layout_height="wrap_content"
        android:layout_marginVertical="8dp"
        android:layout_marginBottom="40dp"
        android:background="@drawable/customshapetwo"
        android:elevation="3dp"
        android:fontFamily="@font/cextrabold"
        android:gravity="center"
        android:hint="SHARING-PIN"
        android:inputType="text"
        android:paddingStart="16dp"
        android:paddingTop="16dp"
        android:paddingEnd="16dp"
        android:paddingBottom="16dp"
        android:textColor="#c4c4c4"
        android:textColorHint="#c4c4c4"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@id/guideline15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/guideline15"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.9" />


</androidx.constraintlayout.widget.ConstraintLayout>